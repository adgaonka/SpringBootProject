<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.springbootproject.persistence.read.VehicleDetailsReadMapper" >

	<resultMap id = "userResult" type = "com.example.springbootproject.model.User">
	   <result property = "id" column = "id"/>
	   <result property = "firstName" column = "first_name"/>
	   <result property = "lastName" column = "last_name"/>
	</resultMap>

	<select id = "getUserInfo" resultMap = "userResult">
	   SELECT * FROM TBL_USERS where id = 1
	</select>
    
</mapper>